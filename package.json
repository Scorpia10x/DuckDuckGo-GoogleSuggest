  },
  "dependencies": {
    "newrelic": "~1.1.1",
    "gatling": "~1.0.1",
    "async": "~0.2.9",
    "mime": "~1.2.11"
  },
  "devDependencies": {
    "js-beautify": "~1.4.2",
    "jshint": "~2.3.0"
  }
}
