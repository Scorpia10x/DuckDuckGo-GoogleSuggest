{
  "name": "node-bang-suggest",
  "version": "1.1.0",
  "engines": {
    "node": "0.10.x",
    "npm": "1.x"
  },
  "jshintConfig": {
    "undef": true,
    "unused": true,
    "newcap": true,
    "node": true,
    "browser": false,
    "devel": true
  },
  "scripts": {
    "start": "gatling ./app.js",
